; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Brickx"
run/main_scene="res://StartMenu.tscn"
config/quit_on_go_back=false
config/features=PackedStringArray("4.3", "GL Compatibility")
run/max_fps=30
config/icon="res://icon.svg"

[display]

window/size/viewport_width=396
window/size/viewport_height=396

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[gui]

theme/custom_font="res://Geist_otf/Geist-Black.otf"

[input_devices]

pointing/android/enable_pan_and_scale_gestures=true

[layer_names]

2d_physics/layer_1="Player"
2d_physics/layer_2="Plattform middle"
2d_physics/layer_3="Plattform right"
2d_physics/layer_4="Plattform left"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
